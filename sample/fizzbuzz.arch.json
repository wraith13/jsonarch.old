{
    "$ref": "https://raw.githubusercontent.com/wraith13/jsonarch/master/template-json-schema.json#",
    "$arch": "template",
    "default":
    {
        "parameter": { "length": 100 }
    },
    "return":
    {
        "$arch": "loop",
        "loop":
        {
            "$arch": "match",
            "parameter":
            {
                "$arch": "call", "refer": [ "number", "sum" ],
                "parameter":
                [
                    { "$arch": "value", "refer": [ "$loop", "index" ] },
                    1
                ]
            },
            "cases":
            [
                {
                    "case":
                    {
                        "if":
                        {
                            "$arch": "call",
                            "refer": [ "object", "equal" ],
                            "parameter":
                            [
                                {
                                    "$arch": "call",
                                    "refer": [ "number", "compare" ],
                                    "parameter":
                                    [
                                        100,
                                        {
                                            "$arch": "value",
                                            "refer": [ "$loop", "index" ]
                                        }
                                    ]
                                },
                                "<"
                            ]
                        }
                    },
                    "return":
                    {
                        "continue": false
                    }
                },
                {
                    "case":
                    {
                        "if":
                        {
                            "$arch": "call",
                            "refer": [ "object", "equal" ],
                            "parameter":
                            [
                                {
                                    "$arch": "call", "refer": [ "number", "remainder" ],
                                    "parameter":
                                    [
                                        { "$arch": "value", "refer": [ "parameter" ] },
                                        15
                                    ]
                                },
                                0
                            ]
                        }
                    },
                    "return":
                    {
                        "return": "FizzBuzz"
                    }
                },
                {
                    "case":
                    {
                        "if":
                        {
                            "$arch": "call",
                            "refer": [ "object", "equal" ],
                            "parameter":
                            [
                                {
                                    "$arch": "call", "refer": [ "number", "remainder" ],
                                    "parameter":
                                    [
                                        { "$arch": "value", "refer": [ "parameter" ] },
                                        3
                                    ]
                                },
                                0
                            ]
                        }
                    },
                    "return":
                    {
                        "return": "Fizz"
                    }
                },
                {
                    "case":
                    {
                        "if":
                        {
                            "$arch": "call",
                            "refer": [ "object", "equal" ],
                            "parameter":
                            [
                                {
                                    "$arch": "call", "refer": [ "number", "remainder" ],
                                    "parameter":
                                    [
                                        { "$arch": "value", "refer": [ "parameter" ] },
                                        5
                                    ]
                                },
                                0
                            ]
                        }
                    },
                    "return":
                    {
                        "return": "Buzz"
                    }
                },
                {
                    "return":
                    {
                        "return": { "$arch": "value", "refer": [ "parameter" ] }
                    }
                }
            ]
        }
    }
}
