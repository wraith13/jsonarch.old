{
    "$ref": "https://raw.githubusercontent.com/wraith13/jsonarch/master/template-json-schema.json#",
    "$arch": "library",
    "object": {
        "$arch": "library",
        "typeOf": {
            "$arch": "template",
            "type": {
                "parameter": {
                    "$arch": "type",
                    "type": "any"
                },
                "return": { "$arch": "type", "type": "type" }
            }
        },
        "equal": {
            "$arch": "template",
            "type": {
                "parameter": {
                    "$arch": "type",
                    "type": "array",
                    "itemType": { "$arch": "type", "type": "any" },
                    "minLength": 2,
                    "maxLength": 2,
                    "lazyable": false
                },
                "return": { "$arch": "type", "type": "boolean" }
            }
        }
    },
    "array": {
        "$arch": "library",
        "contain": {
            "$arch": "template",
            "type": {
                "parameter": {
                    "$arch": "type",
                    "type": "tuple",
                    "list": [
                        {
                            "$arch": "type",
                            "type": "array",
                            "itemType": { "$arch": "type", "type": "any" }
                        },
                        { "$arch": "type", "type": "any" }
                    ]
                },
                "return": { "$arch": "type", "type": "boolean" }
            }
        },
        "map": {
            "$arch": "template",
            "type": {
                "parameter": {
                    "$arch": "type",
                    "type": "object",
                    "member": {
                        "array": {
                            "$arch": "type",
                            "type": "array",
                            "itemType": { "$arch": "type", "type": "any" }
                        },
                        "template": {
                            "$arch": "type",
                            "type": "template"
                        }
                    }
                },
                "return": {
                    "$arch": "type",
                    "type": "array",
                    "itemType": { "$arch": "type", "type": "any" }
                }
            }
        }
    },
    "boolean": {
        "$arch": "library",
        "not": {
            "$arch": "template",
            "type": {
                "parameter": { "$arch": "type", "type": "boolean", "lazyable": false },
                "return": { "$arch": "type", "type": "boolean" }
            }
        },
        "or": {
            "$arch": "template",
            "type": {
                "parameter": {
                    "$arch": "type",
                    "type": "array",
                    "itemType": { "$arch": "type", "type": "boolean" },
                    "lazyable": false
                },
                "return": { "$arch": "type", "type": "boolean" }
            }
        },
        "and": {
            "$arch": "template",
            "type": {
                "parameter": {
                    "$arch": "type",
                    "type": "array",
                    "itemType": { "$arch": "type", "type": "boolean" },
                    "lazyable": false
                },
                "return": { "$arch": "type", "type": "boolean" }
            }
        },
        "xor": {
            "$arch": "template",
            "type": {
                "parameter": {
                    "$arch": "type",
                    "type": "array",
                    "itemType": { "$arch": "type", "type": "boolean" },
                    "minLength": 2,
                    "maxLength": 2,
                    "lazyable": false
                },
                "return": { "$arch": "type", "type": "boolean" }
            }
        }
    },
    "number": {
        "$arch": "library",
        "compare": {
            "$arch": "template",
            "type": {
                "parameter": {
                    "$arch": "type",
                    "type": "array",
                    "itemType": { "$arch": "type", "type": "number" },
                    "minLength": 2,
                    "maxLength": 2,
                    "lazyable": false
                },
                "return": { "$arch": "type", "type": "string", "enum": ["<", "=", ">"] }
            }
        },
        "sum": {
            "$arch": "template",
            "type": [
                {
                    "parameter": {
                        "$arch": "type",
                        "type": "array",
                        "itemType": { "$arch": "type", "type": "number", "integerOnly": true, "minValue": 0 },
                        "lazyable": false
                    },
                    "return": { "$arch": "type", "type": "number", "integerOnly": true, "minValue": 0 }
                },
                {
                    "parameter": {
                        "$arch": "type",
                        "type": "array",
                        "itemType": { "$arch": "type", "type": "number", "minValue": 0 },
                        "lazyable": false
                    },
                    "return": { "$arch": "type", "type": "number", "minValue": 0 }
                },
                {
                    "parameter": {
                        "$arch": "type",
                        "type": "array",
                        "itemType": { "$arch": "type", "type": "number", "integerOnly": true },
                        "lazyable": false
                    },
                    "return": { "$arch": "type", "type": "number", "integerOnly": true }
                },
                {
                    "parameter": {
                        "$arch": "type",
                        "type": "array",
                        "itemType": { "$arch": "type", "type": "number" },
                        "lazyable": false
                    },
                    "return": { "$arch": "type", "type": "number" }
                }
            ]
        },
        "remainder": {
            "$arch": "template",
            "type": {
                "parameter": {
                    "$arch": "type",
                    "type": "tuple",
                    "list": [
                        { "$arch": "type", "type": "number", "minValue": 0, "integerOnly": true },
                        { "$arch": "type", "type": "number", "minValue": 1, "integerOnly": true }
                    ],
                    "lazyable": false
                },
                "return": { "$arch": "type", "type": "number", "minValue": 0, "integerOnly": true }
            }
        }
    },
    "string": {
        "$arch": "library",
        "join": {
            "$arch": "template",
            "type": [
                {
                    "parameter": {
                        "$arch": "type",
                        "type": "array",
                        "itemType": { "$arch": "type", "type": "string" },
                        "lazyable": false
                    },
                    "return": { "$arch": "type", "type": "string" }
                },
                {
                    "parameter": {
                        "$arch": "type",
                        "type": "object",
                        "member": {
                            "list": { "$arch": "type", "type": "array", "itemType": "string" },
                            "separator": { "$arch": "type", "type": "string" }
                        },
                        "lazyable": false
                    },
                    "return": { "$arch": "type", "type": "string" }
                }
            ]
        },
        "split": {
            "$arch": "template",
            "type": {
                "parameter": {
                    "$arch": "type",
                    "type": "object",
                    "member": {
                        "text": { "$arch": "type", "type": "string" },
                        "separator": { "$arch": "type", "type": "string" }
                    },
                    "lazyable": false
                },
                "return": { "$arch": "type", "type": "array", "itemType": "string" }
            }
        }
    }
}
