{
    "$schema": "https://raw.githubusercontent.com/wraith13/jsonarch/master/json-schema/setting-json-schema.json#",
    "$arch": "setting",
    "process": {
        "lazyEvaluation": true
    },
    "limit": {
        "processTimeout": 1000,
        "maxCallNestDepth": 16,
        "maxArrayLength": 1000,
        "maxObjectNestDepth": 32,
        "maxObjectMembers": 1000
    },
    "outputFormat": {
        "digest": {
            "minTargetSize": 100000,
            "maxStringLength": 128,
            "maxObjectNestDepth": 12,
            "maxArrayLength": 32,
            "maxObjectMembers": 32
        }
    }
}
