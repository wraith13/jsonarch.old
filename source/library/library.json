{
    "$ref": "https://raw.githubusercontent.com/wraith13/jsonarch/master/template-json-schema.json#",
    "$arch": "library",
    "object":
    {
        "$arch": "library",
        "equal":
        {
            "$arch": "template",
            "type":
            {
                "parameter":
                {
                    "$arch": "type",
                    "type": "array",
                    "itemType": { "$arch": "type", "type": "any" },
                    "minLength": 2,
                    "maxLength": 2
                },
                "return": { "$arch": "type", "type": "boolean" }
            }
        }
    },
    "boolean":
    {
        "$arch": "library",
        "not":
        {
            "$arch": "template",
            "type":
            {
                "parameter": { "$arch": "type", "type": "boolean" },
                "return": { "$arch": "type", "type": "boolean" }
            }
        },
        "or":
        {
            "$arch": "template",
            "type":
            {
                "parameter":
                {
                    "$arch": "type",
                    "type": "array",
                    "itemType": { "$arch": "type", "type": "boolean" }
                },
                "return": { "$arch": "type", "type": "boolean" }
            }
        },
        "and":
        {
            "$arch": "template",
            "type":
            {
                "parameter":
                {
                    "$arch": "type",
                    "type": "array",
                    "itemType": { "$arch": "type", "type": "boolean" }
                },
                "return": { "$arch": "type", "type": "boolean" }
            }
        },
        "xor":
        {
            "$arch": "template",
            "type":
            {
                "parameter":
                {
                    "$arch": "type",
                    "type": "array",
                    "itemType": { "$arch": "type", "type": "boolean" },
                    "minLength": 2,
                    "maxLength": 2
                },
                "return": { "$arch": "type", "type": "boolean" }
            }
        }
    },
    "number":
    {
        "$arch": "library",
        "compare":
        {
            "$arch": "template",
            "type":
            {
                "parameter":
                {
                    "$arch": "type",
                    "type": "array",
                    "itemType": { "$arch": "type", "type": "number" },
                    "minLength": 2,
                    "maxLength": 2
                },
                "return": { "$arch": "type", "type": "string", "enum": [ "<", "=", ">" ] }
            }
        }
    },
    "string":
    {
        "$arch": "library",
        "join":
        {
            "$arch": "template",
            "type":
            [
                {
                    "parameter": { "$arch": "type", "type": "array", "itemType": { "$arch": "type", "type": "string" } },
                    "return": { "$arch": "type", "type": "string" }
                },
                {
                    "parameter":
                    {
                        "$arch": "type",
                        "type": "object",
                        "member":
                        {
                            "list": { "$arch": "type", "type": "array", "itemType": "string" },
                            "separator": { "$arch": "type", "type": "string" }
                        }
                    },
                    "return": { "$arch": "type", "type": "string" }
                }
            ]
        },
        "split":
        {
            "$arch": "template",
            "type":
            {
                "parameter":
                {
                    "$arch": "type",
                    "type": "object",
                    "member":
                    {
                        "text": { "$arch": "type", "type": "string" },
                        "separator": { "$arch": "type", "type": "string" }
                    }
                },
                "return": { "$arch": "type", "type": "array", "itemType": "string" }
            }
        }
    }
}
